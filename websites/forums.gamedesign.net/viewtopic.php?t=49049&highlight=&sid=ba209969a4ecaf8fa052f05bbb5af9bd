				<td colspan="2"><span class="postbody">some code i wrote for a msn buddy to teach him programming a while ago (that's why the offensivenes are there)
<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">#include &lt;iostream.h&gt; 
<br />
#include &lt;time.h&gt;
<br />
#include &lt;stdlib.h&gt; 
<br />
#include &lt;string.h&gt;
<br />

<br />
#define myRandom&#40;a,limit&#41; srand&#40;&#40;unsigned int&#41;time&#40;NULL&#41;&#41;; a = &#40;rand&#40;&#41; % limit&#41;;
<br />

<br />
int main&#40;&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;// TROY ALWAYS DECLARE VARIABLES BEFORE USING FUNCTIONS !!!
<br />
&nbsp; &nbsp;// SOME COMPILERS HAVE A PROBLEM WHEN A FUNCTION COMES AND THEN A VARIABLE IS DECLARED
<br />
&nbsp; &nbsp;//cout &lt;&lt; &quot;Please pick a number between 1 and 10 \n&quot;; &lt;- THE MISTAKE LA MAKE !
<br />
&nbsp; 
<br />
&nbsp; &nbsp; int enterednumber;
<br />
&nbsp; &nbsp; int rightnumber; 
<br />
&nbsp; &nbsp;char doyouplayagain&#91;2&#93;;
<br />

<br />
playagain&#58;
<br />

<br />
&nbsp; &nbsp; myRandom&#40;rightnumber,10&#41;;
<br />

<br />
&nbsp; &nbsp;while &#40;rightnumber == 0&#41;
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;myRandom&#40;rightnumber,10&#41;; // IF IT's A 0 THEN GENERATE IT AGAIN UNTIL IT's not 0
<br />
&nbsp; &nbsp;&#125;
<br />

<br />
&nbsp; &nbsp;cout &lt;&lt; &quot;Please pick a number between 1 and 10 \n&quot;; // THIS IS THE RIGHT PLACE TO PUT IT
<br />
&nbsp; &nbsp; cin &gt;&gt; enterednumber;
<br />

<br />
&nbsp; &nbsp; if &#40; enterednumber == rightnumber &#41; // IT's enterednumber DUMBASS !
<br />
&nbsp; &nbsp; &#123;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; cout &lt;&lt; &quot;Congratulations!\n&quot; &lt;&lt;rightnumber &lt;&lt;&quot; was the correct number\n&quot;; //ALWAYS CLOSE THE &quot;'s YOU START ! 
<br />
&nbsp; &nbsp;&nbsp; &nbsp;//YOU ALSO FORGOT TO PUT THE &lt;&lt; BEFORE THE SECOND STRING !
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; 
<br />
&nbsp; &nbsp;else 
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;cout &lt;&lt; &quot;I'm sorry.\n&quot; &lt;&lt;rightnumber &lt;&lt;&quot; was the corect number.\n&quot;; // IT's rightnumber DUMBASS ! YOU ARE CHECKING TO SEE IF ENTEREDNUMBER IS SAME AS RIGHTNUMBER !
<br />
&nbsp; &nbsp;&nbsp; &nbsp;//YOU MESSED UP MY CODE TROY,THE THING YOU POSTED WHOULDN'T COMPILE ! I ALSO MADE A COUPLE CORRECTIONS ADDING SPACES AND NEWLINES IN COUT
<br />
&nbsp; &nbsp;&#125;
<br />

<br />
askagain&#58;
<br />

<br />
&nbsp; &nbsp;cout &lt;&lt; &quot;Do you want to play again ? y/n\n&quot;;
<br />
&nbsp; &nbsp;cin &gt;&gt; doyouplayagain;
<br />

<br />
&nbsp; &nbsp;if &#40;!strcmp&#40;doyouplayagain,&quot;y&quot;&#41; | !strcmp&#40;doyouplayagain,&quot;Y&quot;&#41;&#41;
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;goto playagain;
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;else if &#40;!strcmp&#40;doyouplayagain,&quot;n&quot;&#41; | !strcmp&#40;doyouplayagain,&quot;N&quot;&#41;&#41;
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;cout &lt;&lt; &quot;If you don't want to play again,then fuck off ! \n&quot;;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;return 0; // if this is done in the function main this exits the program
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;goto askagain;
<br />
&nbsp; &nbsp;&#125;
<br />

<br />
&nbsp; &nbsp;return 0; // exit the program,although it never gets here
<br />
&#125;
