&nbsp; &nbsp; &nbsp; &nbsp; int d8Amount = 0;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; cout &lt;&lt; &quot;How many D8's do you wish to roll?\n&quot;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; cin &gt;&gt; d8Amount;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; int d8 = 0;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; int loop8;
<br />
